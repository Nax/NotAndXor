<!doctype html>
<html lang="en-US">
  <head>
    {{#if title}}
      <title>{{title}} &mdash; Not And Xor</title>
    {{/if}}
    {{#unless title}}
      <title>Not And Xor</title>
    {{/unless}}
    {{#if canonicalUrl}}
      <link rel="canonical" href="{{canonicalUrl}}"/>
    {{/if}}
    {{#each javascript.scripts}}
      <script nomodule defer="defer" src="{{this}}"></script>
    {{/each}}
    {{#each javascript.modules}}
      <script type="module" defer="defer" src="{{this}}"></script>
    {{/each}}
    {{#each scriptsInline}}
      <script>{{{this}}}</script>
    {{/each}}
    {{#each stylesheets}}
      <link type="text/css" rel="stylesheet" href="{{this}}"/>
    {{/each}}
    {{#if favicon}}
      {{{ favicon }}}
    {{/if}}
  </head>
  <body>
    {{#each ld}}
      <script type="application/ld+json">{{{this}}}</script>
    {{/each}}
  </body>
</html>
